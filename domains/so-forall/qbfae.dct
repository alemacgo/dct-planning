(so-forall (?A0 1 @isa0)
	(so-exists (?E0 1 @ise0)
	    (forall (?c @cls)
	        (exists (?x @var)
	            (or
	                (and (?P ?x ?c) (?E0 ?x))
	                (and (?P ?x ?c) (?A0 ?x))
	                (and (?N ?x ?c) (not (?E0 ?x)))
	                (and (?N ?x ?c) (not (?A0 ?x)))
	            )
	        )
	    )
	)
)    
