(so-forall (?T 1)
	(so-exists (?R 1)
	    (forall (?c)
	        (exists (?x)
	            (or
	                (and (?P ?x ?c)
	                     (or (and (not (?E ?x)) (?T ?x)) (and (?E ?x) (?R ?x)))
	                )
	                (and (?N ?x ?c)
	                     (or (and (not (?E ?x)) (not (?T ?x))) (and (?E ?x) (not (?R ?x))))
	                )
	            )
	        )
	    )
	)
)    
