(so-forall (?T 1 @ist)
	(so-exists (?R 1 @isr)
	    (forall (?c @cla)
	        (exists (?x @var)
	            (or
	                (and (?P ?x ?c) (or (?T ?x) (?R ?x))
	                )
	                (and (?N ?x ?c) (or (not (?T ?x)) (not (?R ?x)))
	                )
	            )
	        )
	    )
	)
)    
