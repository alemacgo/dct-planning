#! /usr/bin/env bash
for i in `ls domains/ | grep .dct | sed -e s/.dct$//`; do    
    mkdir -p problems/$i

    tool/soplan.py --output=problems/$i/$i.pddl domains/$i.dct\
    > /dev/null
        echo "Created domain for $i"
done
mkdir -p solutions
./scripts/so-exists/get_problem_directories.py
./scripts/so-exists/create_problems.py
